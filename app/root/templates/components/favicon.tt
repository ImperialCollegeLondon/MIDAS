[%# this is a template for building the tags necessary to get all of the
  # various types of favicons working. The grunt.usemin task will re-write
  # the tags and add versions to the filenames, but each one has to be
  # done separately, hence the "build" blocks around each one
  # jt6 20150501 WTSI %]

<!-- build:ati /apple-touch-icon-57x57.png -->
<link rel="apple-touch-icon" sizes="57x57" href="/apple-touch-icon-57x57.png">
<!-- endbuild -->
