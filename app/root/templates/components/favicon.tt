[%# NOTE: the grunt.usemin task won't correctly rev the meta tags for
  # the MS tiles unless the "content" attribute is given as an "href"
  # or "src" tag. That's fine, however, because here's a function in
  # the Gruntfile.js which fixes it, re-writing "href" back to "content" %]
<!-- build:tile /mstile-70x70.png -->
<meta name="msapplication-TileImage" href="/mstile-70x70.png">
<!-- endbuild -->
<!-- build:tile /mstile-144x144.png -->
<meta name="msapplication-TileImage" href="/mstile-144x144.png">
<!-- endbuild -->
<!-- build:tile /mstile-150x150.png -->
<meta name="msapplication-TileImage" href="/mstile-150x150.png">
<!-- endbuild -->
<!-- build:tile /mstile-310x150.png -->
